{
  "name": "@naverpay/browserslist-config",
  "version": "2.1.1",
  "description": "Shareable Browserslist config for naverpay",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "README.ko.md",
    "CHANGELOG.md"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./userAgentCheck": {
      "import": {
        "types": "./dist/userAgentCheck.d.mts",
        "default": "./dist/userAgentCheck.mjs"
      },
      "require": {
        "types": "./dist/userAgentCheck.d.ts",
        "default": "./dist/userAgentCheck.js"
      }
    }
  },
  "scripts": {
    "release": "changeset publish",
    "changeset-version": "changeset version",
    "build": "tsup",
    "test": "jest"
  },
  "author": "Naverpay Frontend",
  "license": "ISC",
  "packageManager": "pnpm@9.12.3",
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@types/browserslist-useragent": "^3.0.7",
    "@types/jest": "^30.0.0",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "browserslist": "^4.25.4",
    "browserslist-useragent": "^4.0.0"
  }
}
